{
    "type": "origins:multiple",
    "cooldown": {
        "type": "origins:resource",
        "min": 0,
        "max": 60,
        "hud_render": {
            "should_render": true,
            "condition": {
                "type": "origins:resource",
                "resource": "*:*_cooldown",
                "compare_to": 0,
                "comparison": ">"
            },
			"sprite_location": "origins:textures/gui/community/huang/resource_bar_02.png",
			"bar_index": 1
        }
    },
    "state": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0,
        "min_action": {
            "type": "origins:modify_resource",
            "resource": "*:*_cooldown",
            "modifier": {
                "operation": "set_total",
                "value": 60
            }
        },
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/community/huang/resource_bar_02.png",
            "bar_index": 12,
            "condition": {
                "type": "origins:resource",
                "resource": "*:*_state",
                "compare_to": 0,
                "comparison": ">"
            }
        }
    },
    "active": {
        "type": "origins:active_self",
        "key": "key.origins.secondary_active",
        "hud_render": {
            "should_render": false
        },
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_cooldown",
            "compare_to": 0,
            "comparison": "=="
        },
        "entity_action": {
            "type": "origins:modify_resource",
            "resource": "*:*_state",
            "modifier": {
                "operation": "set_total",
                "value": 1
            }
        }
    },
    "tick_cooldown": {
        "type": "origins:action_over_time",
        "interval": 20,
        "entity_action": {
            "type": "origins:modify_resource",
            "resource": "*:*_cooldown",
            "modifier": {
                "operation": "add_base_early",
                "value": -1
            }
        }
    },
    "dodge": {
        "type": "origins:modify_damage_taken",
        "modifier": {
            "operation": "set_total",
            "value": 0
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "otherworldorigins:leveled_chance",
                    "base": 0.0,
                    "per_level": 0.02,
                    "aptitude": "justlevelingfork:dexterity"
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_state",
                    "compare_to": 1,
                    "comparison": "=="
                }
            ]
        },
        "self_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:entity.enderman.teleport ambient @a ~ ~ ~ 1 1"
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:end_rod ~ ~1.5 ~ .4 .4 .4 0 15 normal"
                }
            ]
        }
    },
    "reduce": {
        "type": "origins:modify_damage_taken",
        "modifier": {
            "operation": "multiply_total",
            "value": -0.5
        },
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_state",
            "compare_to": 1,
            "comparison": "=="
        },
        "self_action": {
            "type": "origins:modify_resource",
            "resource": "*:*_state",
            "modifier": {
                "operation": "set_total",
                "value": 0
            }
        }
